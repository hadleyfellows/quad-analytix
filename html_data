<script type="text/javascript">
var playing=true;
function pauseEspot(e) {
	var keyCode = e.which || e.keyCode;
	var target = e.target;
	if (keyCode == 13 || target == document.getElementById("pause-button")) {
        if (playing) {
            $('#cloudslider').cloudPause();
            playing = false;
            $('a#pause-button')[0].innerHTML = 'Resume';
        } else {
            $('#cloudslider').cloudResume();
            playing = true;
            $('a#pause-button')[0].innerHTML = 'Pause';
        }
    }
}

