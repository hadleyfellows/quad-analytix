<script type="text/javascript">
	dojo.require("wc.render.common");
	wc.render.declareRefreshController({
		id: "ShowMoreButtonController",
		renderContext: wc.render.getContextById("product_listing_container"),
		url: "http://www.scheels.com/shop/ScheelsProductListingView?searchTermScope=&amp;searchType=1000&amp;filterTerm=&amp;langId=-1&amp;advancedSearch=&amp;sType=SimpleSearch&amp;gridPosition=&amp;metaData=&amp;manufacturer=&amp;ajaxStoreImageDir=%2Fwcsstore%2FScheelsStorefrontAssetStore%2F&amp;resultCatEntryType=&amp;catalogId=10051&amp;searchTerm=&amp;resultsPerPage=20&amp;emsName=Widget_ScheelsCatalogEntryList_3461&amp;facet=&amp;categoryId=85168&amp;storeId=10151&amp;enableSKUListView=false&amp;disableProductCompare=false&amp;ddkey=ProductListingView_5_1557_3461&amp;filterFacet=",
		formId: ""
		,refreshHandler: function(widget, data) 
		{
			 var controller = this;
			 var currentIdValue = currentId;
			 dojo.place(data, "show_more_button","replace");
			 //dojo.byId("show_more_button").innerHTML = data;
		}
		,renderContextChangedHandler: function(message, widget) {
			var controller = this;
			var renderContext = this.renderContext;
			console.log(renderContext.properties);  			
			var resultType = renderContext.properties["resultType"];
			if(resultType == "products" || resultType == "both")
			{
				renderContext.properties["beginIndex"] = renderContext.properties["productBeginIndex"];
				widget.refresh(renderContext.properties);
			}
		}
		,postRefreshHandler: function(widget) {
			var controller = this;
			var renderContext = this.renderContext;
			$(".productListingGridAdd").attr("style", $("#defaultListingGrid").attr("style"));
			cursor_clear();
		}
	});
</script>


<div dojotype="wc.widget.RefreshArea" class="dijitContentPane" id="ShowMoreButton" widgetid="ShowMoreButton" controllerid="ShowMoreButtonController" role="wairole:region" waistate:live="polite" waistate:atomic="true" waistate:relevant="all" title="">
	
		<div class="button_align" id="show_more_button">
							
