<script type="text/javascript">
	$(document).ready(function(){
		var currentPage = window.location.href;
		console.log(currentPage);

		if(currentPage.indexOf("OrderShippingBillingView") >= 0 || currentPage.indexOf("MultipleShipmentOrderSummaryView") >= 0 || currentPage.indexOf("UnregisteredCheckoutView") >= 0) {
			$('#footerEmailSection, #footerSocialSection, #footerVisaSection, #footerlinks').css('display', 'none');	

			$('#simplefooterlinks').addClass('simplefooter');
			$('#footerRow1').addClass('simplefooter');
			$('#footerguarantee').addClass('simplefooter');
			$('#footerguarantee p').addClass('simplefooter');
			$('#footerguarantee img').addClass('simplefooter');
			$('#footerRow2').addClass('simplefooter');
			$('#copyright').addClass('simplefooter');
			
		}

		else {
			$('#shippingSpot, #search_quickLinks, #headerRow2wrapper').css('display', 'block');
		}
	});
</script>





<script type="text/javascript">
	dojo.addOnLoad(function() { 
		//Make sure page is loaded at this point
		//Set requestedSubmitted to false
		resetRequest();

		// All div's whose id attribute contains dojoWidget subString -- dojo.query('div[id*="dojoWidget"]')
		// All div's which contains dojoType attribute -- dojo.query(div[dojoType])
		dojo.query('div[dojoType]').forEach(function(node, index, arr){
			addToWidgetsList(node.id);
		});
		parseAllWidgets();
	}); 	
</script><!-- END Footer.jsp -->
